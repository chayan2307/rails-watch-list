<div class="container">
  <h1><%= @list.name %></h1>

  <ul>
    <% @list.movies.each do |movie| %>
      <li><%= movie.title %></li>
    <% end %>
  </ul>
  <%= link_to "Create Bookmark", new_list_bookmark_path(@list) %>
  <br>
  <%= link_to "Delete Bookmark", bookmark_path(@list) %>
  <br>
  <%= link_to "Back to list", lists_path %>
</div>
